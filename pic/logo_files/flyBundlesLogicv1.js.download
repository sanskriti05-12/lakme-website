      getBundleType: function (bundleId) {
        try{
          // get first 3 letters of bundleId
          let bundleType = bundleId.substring(0, 3);
          if(bundleType == "VOL"){
            return "VOLUME_DISCOUNT";
          }

          return "FREQUENT_BOUGHT";

        }catch(err){
          gfb.utility.debugConsole("error in getBundleType => ", err);
        }
